# Azure PostgreSQL Configuration
PGHOST=reveriesdb.postgres.database.azure.com
PGUSER=hperkins
PGPORT=5432
PGDATABASE=reveries
PGPASSWORD=your-password-here

# Vite Environment Variables for Database Connection
VITE_DB_HOST=reveriesdb.postgres.database.azure.com
VITE_DB_USER=hperkins
VITE_DB_PORT=5432
VITE_DB_NAME=reveries
VITE_DB_PASSWORD=your-password-here

# API Keys (add your actual keys here)
VITE_GOOGLE_AI_API_KEY=your-gemini-api-key
VITE_XAI_API_KEY=your-grok-api-key
VITE_AZURE_OPENAI_API_KEY=your-azure-openai-key
VITE_AZURE_OPENAI_ENDPOINT=your-azure-openai-endpoint

# Search Provider API Keys
VITE_EXA_API_KEY=your-exa-api-key
VITE_GOOGLE_SEARCH_API_KEY=your-google-search-api-key
VITE_GOOGLE_SEARCH_ENGINE_ID=your-google-search-engine-id
VITE_BING_SEARCH_API_KEY=your-bing-search-api-key

# Application Configuration
NODE_ENV=development
VITE_APP_NAME=Reveries Research Assistant
VITE_APP_VERSION=1.0.0

# SSL Configuration for Azure PostgreSQL
VITE_DB_SSL=true
VITE_DB_SSL_MODE=require

# Google AI / Gemini
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# XAI / Grok
VITE_XAI_API_KEY=your_xai_api_key_here

# Azure OpenAI
VITE_AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
VITE_AZURE_OPENAI_API_KEY=your_azure_api_key_here
VITE_AZURE_OPENAI_DEPLOYMENT=o3
VITE_AZURE_OPENAI_API_VERSION=2024-10-01-preview

# Database Configuration
VITE_DB_HOST=localhost
VITE_DB_PORT=5432
VITE_DB_USER=postgres
VITE_DB_NAME=reveries
VITE_DB_PASSWORD=your_db_password_here
VITE_DB_SSL=false
VITE_DB_SSL_MODE=disable

# Application Settings
VITE_APP_NAME=Reveries Research Assistant
VITE_APP_VERSION=0.0.1
VITE_DEFAULT_MODEL=gemini-pro
VITE_ENABLE_FALLBACK=true

# Rate Limiting
VITE_AZURE_OPENAI_CONCURRENCY=5
VITE_MAX_RETRIES=3
VITE_RATE_LIMIT_TOKENS_PER_MINUTE=150000
VITE_RATE_LIMIT_REQUESTS_PER_MINUTE=10000
VITE_REQUEST_TIMEOUT=30000
